    </validationRules>
    <validationRules>
        <fullName>age_limit_between_21_to_30</fullName>
        <active>true</active>
        <errorConditionFormula>IF(OR((Year(Today()) - Year( Date_of_Birth__c )) &lt; 21,(Year(Today()) - Year( Date_of_Birth__c )) &gt; 30),TRUE,FALSE)</errorConditionFormula>
        <errorDisplayField>Date_of_Birth__c</errorDisplayField>
